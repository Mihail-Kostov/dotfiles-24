# ...................................................................... Browser

if [ $1 ] ;then
  /usr/bin/qutebrowser "$@"
else
  ping -c 1 -W 1 luna >/dev/null 2>&1 && TTRSS=luna || TTRSS=localhost
  /usr/bin/qutebrowser http://$TTRSS:8000/tt-rss/
fi

