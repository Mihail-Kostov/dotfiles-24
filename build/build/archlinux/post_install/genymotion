# ................................................................... genymotion

sudo sed -i '$a\
vboxnetflt\
vboxnetadp\
vboxpci\
' /etc/modules-load.d/virtualbox.conf

sudo modprobe vboxnetflt
sudo modprobe vboxnetadp
sudo modprobe vboxpci

