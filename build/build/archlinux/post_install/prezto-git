# tailor dash session login / logout
ifnsed /etc/dash/zlogin 'echo ${_yellow_}' '1iecho ${_yellow_}---'
ifnsed /etc/dash/zlogout '#source .*zlogout' '/^source .*zlogout/s/^/#/'

