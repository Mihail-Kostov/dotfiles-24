[ $TRACE ] && CHILD="focus/instance $@" . $PARENT

# herbstluftwm
# ▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂▂

# ............................................................... Focus instance

# Usage: client <instance>

for i in $(query winids)
do
  if herbstclient compare clients.$i.instance = "$1" ;then
    herbstclient jumpto $i
    return 0
  fi
done
return 1

